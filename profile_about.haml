                %h2.tx.__upper.m-b_2
                  Friends 
                  %span.tx.__400 (321)
                .block
                  .p-t_1.p-b_1
                    .avatars.scroll-pane
                      - 1.upto(100) do
                        %a.avatar{href: "#"}>
