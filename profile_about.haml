                          %p female
                    %div.m-b_1
                      %dl.grid
                        %dt.grid-i.__grid-1-2>
                          %p.tx.__right.__700 City
                        %dd.grid-i.__grid-1-2>
                          %p Moscow
                    %div.m-b_2
                      %dl.grid
                        %dt.grid-i.__grid-1-2>
                          %p.tx.__right.__700 Country
                        %dd.grid-i.__grid-1-2>
