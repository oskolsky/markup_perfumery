                .grid-i.__grid-1-1.m-b_0>
                  - 1.upto(28) do |i|
                    .tabs_page.__border{id: "tab#{i}"}
                      .brands-list
                        - 1.upto(9) do
                          .brands-list_i
                            .grid
                              .grid-i.__grid-1-13>
                                .brands-list_i_group A
                              - 1.upto(6) do
                                .grid-i.__grid-2-13>
                                  %ul.brands-list_i_names
                                    %li
                                      %a{href: "/brands_show.html"} Abercrombie & Fitch
